<Window x:Class="BLP_model.Object_Editor_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="" MinWidth="200" MinHeight="100" Height="300" Width="500" Closing="Window_Closing" Loaded="Window_Loaded">
    <Grid>
        <Button x:Name="Save_button" Content="Сохранить" FontSize="14" HorizontalAlignment="Left" Margin="10,0,0,10" VerticalAlignment="Bottom" Width="75" Click="Save_button_Click"/>
        <Button x:Name="Delete_button" Content="Удалить" FontSize="14" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="75" Click="Delete_button_Click"/>
        <TextBox x:Name="Textbox" HorizontalAlignment="Stretch" TextWrapping="Wrap" VerticalAlignment="Stretch" SpellCheck.IsEnabled="True" RenderTransformOrigin="0,0" FontSize="14" AcceptsTab="True" AcceptsReturn="True" Margin="0,0,0,45"/>
        <Label x:Name="Warning_label" Foreground="Red" Content="" HorizontalAlignment="Stretch" Margin="90,0,89.667,9.667" VerticalAlignment="Bottom" FontSize="14" FontWeight="Bold" FontStyle="Italic" Background="Transparent"/>
    </Grid>
</Window>
